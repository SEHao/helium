{
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/helium/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/helium",
        "index": "apps/helium/src/index.html",
        "main": "apps/helium/src/main.ts",
        "polyfills": "apps/helium/src/polyfills.ts",
        "tsConfig": "apps/helium/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/helium/src/favicon.ico",
          "apps/helium/src/assets"
        ],
        "styles": [
          "apps/helium/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/helium/src/environments/environment.ts",
              "with": "apps/helium/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "helium:build:production"
        },
        "development": {
          "browserTarget": "helium:build:development",
          "proxyConfig": "src/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "helium:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/helium/src/test.ts",
        "polyfills": "apps/helium/src/polyfills.ts",
        "tsConfig": "apps/helium/tsconfig.spec.json",
        "karmaConfig": "apps/helium/karma.conf.js",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/helium/src/favicon.ico",
          "apps/helium/src/assets"
        ],
        "styles": [
          "apps/helium/src/styles.scss",
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
          "node_modules/primeng/resources/primeng.min.css"
        ],
        "scripts": []
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/helium/**/*.ts",
          "apps/helium/**/*.html"
        ]
      }
    }
  }
}
